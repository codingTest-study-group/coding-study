package backjoon;

import java.util.Scanner;

public class ex11047 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int N = sc.nextInt();
        int num = sc.nextInt();
        int[] coin = new int[N];
        int count =0;

        for (int i = 0; i < N; i++) {
            coin[i] = sc.nextInt();
        }

        for (int i = N-1; i >= 0; i--) {
            if(coin[i] <=num){
                count = count + (num/coin[i]);
                num = num % coin[i];
            }
        }
        System.out.println(count);
    }
}
